{
  "env": {
    "es2021": true,
    "commonjs": true
  },
  "extends": [
    "eslint: recommended",
    "plugin: jsx-ally/recommended",
    "plugin: import/recommended",
    "airbnb-base",
    "prettier"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [" js", " js√ó"] }],
    "no-underscore-dangle": ["error", { "allow": ["_id"] }]
  }
}
